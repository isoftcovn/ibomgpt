{
    "name": "runner",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "setup": "yarn install && bundle install && cd ios && bundle exec pod install",
        "generateImages": "node scripts/genimg app/assets/images/resources --output app/assets/GeneratedImages.ts --name GeneratedImages",
        "start": "react-native start",
        "test": "jest",
        "lint": "yarn lint:ts && yarn lint:style",
        "lint:ts": "tsc --noEmit",
        "lint:style": "eslint \"./app/**/*.{js,ts,jsx,tsx}\"",
        "lint:fix": "eslint \"./app/**/*.{js,ts,jsx,tsx}\" --quiet --fix",
        "ar": "npx react-native run-android --mode=productionDebug",
        "ad": "npx react-native run-android --mode=devDebug",
        "adStaging": "npx react-native run-android --mode=stagingDebug",
        "fastlaneStagingEnv": "cp fastlane/.env.staging fastlane/.env && cp fastlane/register_devices_staging.txt fastlane/register_devices.txt && cp fastlane/tester_file_staging.txt fastlane/tester_file.txt",
        "fastlaneDevEnv": "cp fastlane/.env.dev fastlane/.env && cp fastlane/register_devices_dev.txt fastlane/register_devices.txt && cp fastlane/tester_file_dev.txt fastlane/tester_file.txt",
        "fastlaneProductionEnv": "cp fastlane/.env.production fastlane/.env && cp fastlane/register_devices_production.txt fastlane/register_devices.txt && cp fastlane/tester_file_production.txt fastlane/tester_file.txt",
        "ios:tf": "fastlane ios shipTF",
        "ios:adhoc": "fastlane ios shipAdHoc",
        "android:ship": "fastlane android ship",
        "certificates": "yarn run fastlaneDevEnv && fastlane genCertificates && git stash && yarn run fastlaneStagingEnv && fastlane genCertificates && git stash && yarn run fastlaneProductionEnv && fastlane genCertificates",
        "buildAndroidStaging": "cd android && ./gradlew assembleStagingRelease",
        "buildAndroidDev": "cd android && ./gradlew assembleDevRelease",
        "buildAndroid": "cd android && ./gradlew assembleProductionRelease",
        "publishAndroid": "yarn run android:ship",
        "publishIOS": "yarn run ios:adhoc",
        "publishIOSTF": "yarn run ios:tf",
        "publishApp": "yarn run publishAndroid && yarn run publishIOS",
        "publishAppTF": "yarn run publishAndroid && yarn run publishIOSTF",
        "postinstall": "patch-package && react-native-schemes-manager all"
    },
    "xcodeSchemes": {
        "Debug": [
            "Debug.Staging",
            "Debug.Dev"
        ],
        "Release": [
            "Release.Staging",
            "Release.Dev"
        ]
    },
    "projectDirectory": "ios",
    "dependencies": {
        "@bam.tech/react-native-image-resizer": "^3.0.7",
        "@baronha/react-native-multiple-image-picker": "^1.1.6",
        "@microsoft/signalr": "^7.0.14",
        "@notifee/react-native": "^7.8.0",
        "@react-native-async-storage/async-storage": "^1.21.0",
        "@react-native-clipboard/clipboard": "^1.12.1",
        "@react-native-community/datetimepicker": "^7.6.2",
        "@react-native-community/netinfo": "^11.1.1",
        "@react-native-community/slider": "^4.4.3",
        "@react-native-firebase/analytics": "^18.7.1",
        "@react-native-firebase/app": "^18.7.1",
