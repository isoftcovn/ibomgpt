        "react-native-config": "^1.5.1",
        "react-native-create-thumbnail": "^1.6.4",
        "react-native-device-info": "^10.12.0",
        "react-native-document-picker": "^9.0.1",
        "react-native-dropdownalert": "^4.5.1",
        "react-native-elements": "^3.4.3",
